{
  "testEnvironment": "node",
  "collectCoverageFrom": ["scripts/**/*.js", "!scripts/**/node_modules/**", "!**/coverage/**"],
  "coverageDirectory": "coverage",
  "coverageReporters": ["text", "lcov", "html"],
  "testMatch": ["**/__tests__/**/*.js", "**/?(*.)+(spec|test).js"],
  "verbose": true,
  "clearMocks": true
}
